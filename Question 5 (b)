#include <iostream>
#include <queue>

using namespace std;
class stack{
    int top1=-1;
    queue <int> q1;
    public:
    void push1(int x){
        q1.push(x);
        int i=1;
        while(i<=q1.size()-1){
            q1.push(q1.front());
            q1.pop();
            i++;

        }

    }
    
    void pop(){
        q1.pop();
    }
    void display(){
        while(!q1.empty()){
            cout<<q1.front()<<" ";
            q1.pop();
        }
        cout<<endl;
    }

};

int main(){
    stack a;
    a.push1(1);
    a.push1(2);
    a.push1(3);
   
    a.display();


}
