#include <iostream>
#include <queue>
#include <stack>
using namespace std;
void binary(int x){
    queue <int> b;
    stack <int> a;
    for(int i=1;i<=x;i++){
        int z=i;
        while(true){
        
            if(z/2==0){
                a.push(z%2);
                break;
            }
            else{
            a.push(z%2);
            z=z/2;}
        
    }
    
    
    while(!a.empty()){
        b.push(a.top());
        a.pop();
    }
    while(!b.empty()){
        cout<<b.front();
        b.pop();
    }
cout<<" ";
    
    }

}
int main(){
binary(8);

    
}
