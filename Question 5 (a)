#include <iostream>
#include <queue>

using namespace std;
class stack{
    int top1=-1;
    queue <int> q1,q2;
    public:
    void push(int x){
        q2.push(x);
        while(!q1.empty()){
            q2.push(q1.front());
            q1.pop();
        }
        swap(q1,q2);

    }
    
    void pop(){
        q1.pop();
    }
    void display(){
        while(!q1.empty()){
            cout<<q1.front()<<" ";
            q1.pop();
        }
        cout<<endl;
    }

};

int main(){
    stack a;
    a.push(1);
    a.push(2);
    a.push(3);
    a.pop();
    a.display();


}
